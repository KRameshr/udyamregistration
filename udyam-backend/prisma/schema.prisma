
datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model UdyamRegistration {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  aadhaarNumber  String
  ownerName      String
  declarationA   Boolean
  createdAt      DateTime @default(now())
}
